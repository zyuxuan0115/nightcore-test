#!/bin/bash
git clone --recurse-submodules https://github.com/zyuxuan0115/nightcore.git /nightcore-new
cd /nightcore-new 
./build_deps.sh && make -j $(nproc)
sleep 20
/nightcore-new/bin/release/launcher \
    --root_path_for_ipc=/tmp/nightcore/ipc \
    --fprocess_mode=cpp \
    --fprocess_output_dir=/nightcore \
    --fprocess="/nightcore/func_worker_v1 $1" \
    --fprocess_output_dir=/tmp/nightcore/output --func_id=$2 \
    --v=1

